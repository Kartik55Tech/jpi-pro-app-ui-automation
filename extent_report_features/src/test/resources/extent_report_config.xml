<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>

    <!-- report theme -->
    <!-- STANDARD, DARK -->
    <theme>DARK</theme>

    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>

    <!-- protool for script and stylesheets -->
    <!-- defaults to https -->
    <!-- HTTP, HTTPS -->
    <protocol>HTTPS</protocol>

    <!-- offline report -->
    <timelineEnabled>true</timelineEnabled>
    
    <!-- offline report -->
    <enableOfflineMode>false</enableOfflineMode>
    
    <!-- use thumbnails for base64 images -->
    <!-- this may slowdown viewing tests -->
    <thumbnailForBase64>false</thumbnailForBase64>

    <!-- title of the document -->
    <documentTitle>Doc Title</documentTitle>

    <!-- report name - displayed at top-nav -->
    <reportName>Report Name</reportName>

    <!-- timestamp format -->
    <timeStampFormat>dd-mm-yyyy hh:mm:ss</timeStampFormat>
    
     <!-- custom javascript -->
    <scripts>
      <![CDATA[
        document.addEventListener('DOMContentLoaded', function() {
          var logoElement = document.getElementsByClassName('logo')[0];
          if (logoElement) {
            logoElement.style.display = 'none';
          }
        });
      ]]>
    </scripts>

    <!-- custom styles -->
    <styles>
      <![CDATA[
        .badge-primary {
          background-color: #f44f04;
        }
      ]]>
    </styles>

  </configuration>
</extentreports>